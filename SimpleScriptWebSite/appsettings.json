{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Sandboxer": {
    "DllFileName": "SimpleScript.Compiler.dll",
    "MaxTotalNumberContainers": 100,
    "AllowedMaxLifeTimeContainerInSeconds": 30,
    "WatcherCheckIntervalInSeconds": 10,
    "MaxRequestsPerMinute": 20,
    "IgnoredOutputs": [
      "Start execution of command execute-direct.",
      "Successfully finished execution of command execute-direct."
    ]
  }
}
